<?xml version="1.0"?>

<robot name="simple_bot">
    <link name="base_link"/>

    <joint name="base_joint" type="fixed">
        <parent link="base_link"/>
        <child link="laser"/>
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    </joint>

    <link name="laser">
        <origin xyz="0.0 0.0 0.0" rpy="0 0 0"/>
    </link>
</robot>
